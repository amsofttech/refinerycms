<h2><%= t('.seo') %></h2>
<div class='field'>
  <%= f.label :browser_title, t('.seo_override_title') %>
  <%= f.text_field :browser_title, :size => 63 %><br/>
  <small>
    <%= t('.seo_override_title_description') %>
  </small>
</div>
<div class='field'>
  <%= f.label :meta_keywords, t('.meta_keywords_title') %>
  <%= f.text_field :meta_keywords, :size => 63 %><br/>
  <small>
    <%= t('.meta_keywords') %>
          </small>
</div>
<div class='field'>
  <%= f.label :meta_description,t('.meta_description_title') %>
  <%= f.text_area :meta_description, :cols => 95, :rows => 3 %><br/>
  <small>
    <%= t('.meta_description') %>
  </small>
</div>
