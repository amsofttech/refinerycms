<div id='actions'>
  <ul>
    <li>
      <%= link_to t(:update_notified, :default => "Update who gets notified"), edit_admin_inquiry_setting_url(InquirySetting.notification_recipients), :class => "user_comment_icon" %>
    </li>
    <li>
      <%= link_to t(:edit_confirmation_email, :default => "Edit confirmation email"), edit_admin_inquiry_setting_url(InquirySetting.confirmation_body), :class => "edit_email_icon" %>
    </li>
  </ul>
</div>
<div id='records'>
  <% unless @inquiry_settings.empty? %>
    <ul>
      <%= render :partial => "inquiry_setting", :collection => @inquiry_settings %>
    </ul>
  <% end -%>
</div>
